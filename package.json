{
  "name": "muscatplus_search_ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "elm-live": "^4.0.2",
    "uglify-js": "^3.12.2"
  },
  "scripts": {
    "develop:search": "elm-live src/SearchApp.elm -u --port 8001 --dir=./dev/search -- --output=dev/search/app.js --debug",
    "develop:records": "elm-live src/RecordsApp.elm -u --port 8002 --dir=./dev/records -- --output=dev/records/app.js --debug",
    "production:search:build": "elm make src/SearchApp.elm --optimize --output=dist/search/app.js",
    "production:records:build": "elm make src/RecordsApp.elm --optimize --output=dist/records/app.js",
    "production:search:deploy": "./optimize.sh src/SearchApp.elm dist/static/search/app.js dist/static/search/app.min.js",
    "production:records:deploy": "./optimize.sh src/RecordsApp.elm dist/static/records/app.js dist/static/records/app.min.js"
  }
}
