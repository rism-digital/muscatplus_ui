  "dependencies": {},
  "devDependencies": {
    "chokidar": "^3.5.1",
    "chokidar-cli": "^2.1.0",
    "elm-live": "^4.0.2",
    "elm-review": "^2.5.2",
    "uglify-js": "^3.12.2"
  },
  "scripts": {
    "develop": "elm-live src/Main.elm -u --port 8002 --dir=./dist -- --output=dist/static/app.js --debug",
